  {
   System.out.print("*");
  }
     else
    {
     System.out.print(" ");
    }
  }
// A letter
System.out.print(" ");
for(int j=0;j<=n-1;j++)
  {
     if((j==0 && i!=0)||(j==n-1 && i!=0)||i==n/2||(i==0 && j!=0  && j!=n-1))
