  }
     else
    {
     System.out.print(" ");
    }
  }
// Space
for(int k=0;k<=4;k++)
{
System.out.print(" ");
}
// G letter
 for(int j=0;j<=n-1;j++)
  {
     if((j==0 || i==0) || (j==n-1 && i!=n/2-1) || (i==n-1 && j<=n/2) || (j==n-1 && i!=n/2-1) || (i==n/2 && j>=n/2) || (j==n/2 && i>=n/2) )
  {
   System.out.print("*");
  }
     else
    {
     System.out.print(" ");
    }
  }
// R letter
System.out.print(" ");
for(int j=0;j<=n-1;j++)
  {
     if((j==0||i-j==n/2)||(i==0 && j!=n/2 && j<=n/2)||(i==n/2&&j!=n/2&&j<=n/2)||(j==n/2 && i!=0 && i<n/2))
  {
   System.out.print("*");
  }
     else
    {
     System.out.print(" ");
    }
  }
// Space
for(int k=0;k<=4;k++)
{
System.out.print(" ");
}
// S letter
for(int j=0;j<=n-1;j++)
  {
     if((i==0 && j!=0)||(j==0 && i!=0 && i<n/2)||(i==n/2 && j!=0 && j!=n-1)||(j==n-1 && i>n/2 && i!=n-1)||(i==n-1 && j!=n-1))
  {
   System.out.print("*");
  }
     else
    {
     System.out.print(" ");
    }
