// K letter
System.out.print(" ");
System.out.print(" ");
for(int j=0;j<=n-1;j++)
  {
     if(j==0 || i-j==n/2 || i+j==n/2) 
  {
   System.out.print("*");
  }
     else
    {
     System.out.print(" ");
    }
  }
// Space
for(int k=0;k<=5;k++);
{
System.out.print(" ");
} 
// Y letter
for(int j=0;j<=n-1;j++)
  {
     if(i+j==n-1||(i==j && j<=n/2))
  {
   System.out.print("*");
  }
     else
    {
     System.out.print(" ");
    }
  }
// O letter
System.out.print(" ");
for(int j=0;j<=n-1;j++)
  {
     if((i==0 && j!=0 && j!=n-1)||(i==n-1 && j!=0 && j!=n-1)||(j==0||j==n-1)&&(i!=0 && i!=n-1))
  {
   System.out.print("*");
  }
     else
    {
     System.out.print(" ");
    }
  }
// U letter
System.out.print(" ");
for(int j=0;j<=n-1;j++)
  {
     if((i==n-1 && j!=0 && j!=n-1)|| (j==0||j==n-1)&&(i!=n-1))
